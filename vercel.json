{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/user-weather",
      "dest": "server.js"
    },
    {
      "src": "/api/alerts",
      "dest": "server.js"
    },
    {
      "src": "/api/weather",
      "dest": "server.js"
    },
    {
      "src": "/api/random-fact",
      "dest": "server.js"
    },
    {
      "src": "/styles.css",
      "dest": "public/styles.css"
    },
    {
      "src": "/White Petals by Keys of Moon.mp3",
      "dest": "public/White Petals by Keys of Moon.mp3"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
